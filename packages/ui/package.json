{
  "name": "@workspace/ui",
  "version": "0.1.5",
  "private": true,
  "description": "UI package",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/ui.mjs"
    },
    "./dist/style.css": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "main": "./dist/ui.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "vite build",
    "check-exports": "npx check-export-map",
    "clean": "rimraf dist node_modules",
    "dev": "vite",
    "lint": "eslint --cache .",
    "preview": "vite preview",
    "typecheck": "tsc"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.2.0"
  },
  "eslintConfig": {
    "extends": "@workspace",
    "rules": {
      "indent": [
        "error",
        2
      ]
    }
  }
}
